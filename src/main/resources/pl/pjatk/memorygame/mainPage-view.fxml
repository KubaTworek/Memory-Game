<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>

<?import javafx.scene.control.Button?>

<VBox alignment="CENTER" spacing="20.0" xmlns:fx="http://javafx.com/fxml"
      fx:controller="pl.pjatk.memorygame.Controller">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
    </padding>

    <Label fx:id="titleText" text="Memory The Game"/>
    <Label fx:id="sNumber" text="s24646"/>
    <Button fx:id="newGameBtn" text="New Game" onAction="#onNewGameBtnClick"/>
    <Button fx:id="highScoresBtn" text="High Scores" onAction="#onHighScoresBtnClick"/>
    <Button fx:id="exitBtn" text="Exit" onAction="#onExitBtnClick"/>

</VBox>

